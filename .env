#
# Enviromental values for Makefile and for docker-compose.yml
#

XDEBUG_ENABLE = true

# Docker Compose project name
COMPOSE_PROJECT_NAME=hel-fi-form-tool

# Local hostname
DRUPAL_HOSTNAME=hel-fi-form-tool.docker.so

# Docker image
DRUPAL_IMAGE=ghcr.io/city-of-helsinki/drupal-web:8.1

# Default source @alias where to sync database and files from
DRUPAL_SYNC_SOURCE=main

# Public webroot
DRUPAL_WEBROOT=public

OC_PROJECT_NAME=hki-kanslia-aok-lomaketyokalu
PROJECT_NAME=hki-kanslia-aok-lomaketyokalu-dev
<div class="submission-print">
  <div class="page-type--form-page wrapper no-sidebar no-sidebar-first no-sidebar-second submission-print__top">
    <header role="banner" class=" submission-print__header">
      <div class="container">
        <div class="submission-print__header-branding"¨>
          <div class="submission-print__branding-block">
            <div class="submission-print__sector-name">
              {{ sector }}
            </div>
            <div class="submission-print__sector-address">
              {{ address|replace({"\n":'<br/>', "\r":''})|raw }}
            </div>
          </div>
          <div class="submission-print__header-branding-right">
            <div>
              <h2 class="submission-print__label">Lomakkeen numero</h2>
              {{ id }}
            </div>
            <div>
              <h2 class="submission-print__label">Lomake lähetetty</h2>
              {{ submissionDate|date('d.m.Y H:i:s') }}
            </div>
            <a type="button" class="hds-button hds-button--primary" href="javascript:window.print();return void;">

                <span aria-hidden="true" class="hel-icon hel-icon--printer"></span>
              <span class="hds-button__label">{{ "Print form"|t }}</span>

            </a>          </div>
        </div>
      </div>
    </header>

  </div>
  <div>


    <main role="main" class="layout-main-wrapper">
      <a id="main-content" tabindex="-1"></a>
      <div class="container">
        <div>
          <div>
          <section class="js-form-item form-item js-form-wrapper form-wrapper webform-section">
            <div class="webform-section-flex-wrapper">
              <h2 class="webform-section-title">Tietoa tästä lomakkeesta</h2>
              <div class="webform-section-wrapper">
                Tietoa
              </div>
            </div>
          </section>
        </div>
          {{ submission }}
        </div>
        <a type="button" class="hds-button hds-button--primary" href="javascript:window.print();return void;">

          <span aria-hidden="true" class="hel-icon hel-icon--printer"></span>
          <span class="hds-button__label">{{ "Print form"|t }}</span>

        </a>
      </div>

    </main>

  </div>
  <footer role="contentinfo" class="footer">
    <div class="container">

    </div>
  </footer>
</div>
